#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t; cin >> t;
	while(t--){
	    int n; cin >> n;
	    stack<int>st;
	    for(int i = 1; i <= n; ++i)st.push(i);
	    
	    while(!st.empty()){
	        cout << st.top() << " ";
	        st.pop();
	    }
	    cout << endl;
	}

}
