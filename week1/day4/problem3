using namespace std;
const int N = 1e5 + 5;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();
    int a, b, t;
    cin >> a >> b >> t;
    double t_t = t + 0.5;
   
    ll total = 0;

    int i = 1;
    int temp = a;

    while (temp < t_t)
    {
        

        if (temp <= t_t)
        {
            total += b;
        }
        
            temp = a;
        ++i;
        temp *= i;
    }
    cout << total << endl;
    return 0;
}
