using namespace std;
const int N = 1e5 + 5;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();
    ll sum = 0;
    int n;
    cin >> n;
    vector<int>v(n);
    for (int i = 0; i < n; ++i)
    {
        cin >> v[i];
        sum+= v[i];
    }
    ll tot = ((ll)n *(n+1))/2;
    // cout << tot << endl;
    // cout << sum << endl;
    cout << tot-sum << endl;
    return 0;
}
