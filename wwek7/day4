using namespace std;
const int N = 1e5 + 7;
void solve()
{
    string s; cin >> s;
    reverse(s.begin() , s.end());//for fun purpose ....its not mendatory
    s=  s.substr(0 , s.size()/2);
    
    ll ans = 0;
    for(int i = 0; i < s.size(); ++i)if(s[i]!= s[i+1])++ans;
    if(ans>1 )cout << "YES" << endl;
    else cout << "NO" << endl;
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
     cin >> t;
    while (t--)
    {
        solve();
    }

    return 0;
}
