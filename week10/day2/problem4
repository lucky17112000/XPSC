#include <bits/stdc++.h>
#define endl '\n'
#define ll long long int
#define inf INT_MAX
#define pii pair<ll, ll>
#define negInf INT_MIN
#define gcd(a, b) __gcd(a, b)
#define lcm(a, b) ((a * b) / gcd(a, b))
