using namespace std;
const int N = 1e5 + 7;
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll n;
    cin >> n;
    vector<pii> arr(n);
    for (int i = 0; i < n; ++i)
    {
        ll val;
        cin >> val;
        arr[i] = {val, i};
    }
    vector<ll> se(n);
    for (int i = 0; i < n; ++i)
    {
        cin >> se[i];
    }
    sort(arr.begin(), arr.end());
    
    vector<ll> ans;
    for (int i = 0; i < n; ++i)
    {
        ans.push_back(se[arr[i].second]);
    }
    for (auto u : arr)
        cout << u.first << " ";
    cout << endl;

    for (auto u : ans)
        cout << u << " ";
    cout << endl;
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
    cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
