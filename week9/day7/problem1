using namespace std;
const int N = 1e5 + 7;
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll n, m;
    cin >> n >> m;
   // ll task = m;
    ll arr[n][n];

    for (int i = 0; i < n; ++i)
    {
        for (int j = 0; j < n; ++j)
        {
            cin >> arr[i][j];
        }
    }
    ll task = 0;

    // row reverse
    for (int i = 0; i < n; ++i)
    {
        for (int j = 0; j < n; ++j)
        {
            if (arr[i][j] != arr[n - i - 1][n - j - 1])
                ++task;
        }
    }
    task /= 2;
    if (m >= task and n % 2 != 0)
    {
        cout << "YES" << endl;
    }
    else if (m >= task)
    {
        if (n % 2 == 0)
        {
            if ((m-task) % 2 == 0)
            {
                cout << "YES" << endl;
            }
            else cout << "NO" << endl;
        }
    }
    else cout << "NO" << endl;


    //puro 4 gonta .......
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
    cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
