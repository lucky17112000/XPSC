using namespace std;
const int N = 1e5 + 5;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    // hey_output();
    string s1, s2;
    cin >> s1 >> s2;
    ll i = 0;
    ll cnt = 0;
    while (i < s1.size())
    {
        if (s1[i] != s2[i])
            ++cnt;
        ++i;
    }
    cout << cnt << endl;

    return 0;
}
