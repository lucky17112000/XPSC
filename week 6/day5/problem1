using namespace std;
const int N = 1e5 + 7;
//1 3 4 10 10
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll n;
    cin >> n;
    vector<ll> arr(n);
    for (int i = 0; i < n; ++i)
        cin >> arr[i];
    sort(arr.begin(), arr.end());
    ll k;
    cin >> k;
    while (k--)
    {
        ll a, b;
        cin >> a >> b;
        auto first = lower_bound(arr.begin(), arr.end(), a);
        auto last = upper_bound(arr.begin() , arr.end() , b);
        cout << last -  first << " ";
        
    }
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
    // cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
