using namespace std;
const int N = 1e5 + 7;
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll n;
    cin >> n;
    vector<ll> arr(n - 1);
    for (int i = 0; i < n - 1; ++i)
        cin >> arr[i];
    vector<ll> ans(n, 0);
    ans[n - 1] = arr[n - 2];
    ll tak = ans[n - 2] = 1e9;
    // cout << take << endl;
    // for(auto u : ans)cout << u << " ";
    
    for (int i = n - 3; i >= 0; --i)
    {
        ans[i] = tak - arr[i];
        tak-=arr[i];

    }
    for(auto u : ans )cout << u << " ";
    cout << endl;

}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
    cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
