using namespace std;
const int N = 1e5 + 7;
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll o, t, th, f;
    cin >> o >> t >> th >> f;
    ll fmi = min(o, t);
    ll fmx = max(o, t);
    bool flagth = false;
    bool flagfo = false;
    for (int i = fmi; i <= fmx; ++i)
    {
        if (i == th)
        {
            flagth = true;
            break;
        }
    }
    for (int i = fmi; i <= fmx; ++i)
    {
        if (i == f)
        {
            flagfo = true;
            break;
        }
    }
    if (flagfo == true and flagth == true)
        cout << "NO" << endl;
    else if (flagfo == false and flagth == false)
        cout << "NO" << endl;
    else if (flagfo == true and flagth == false)
        cout << "YES" << endl;
    else if (flagth == true and flagfo == false)
        cout << "YES" << endl;
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();

    int t = 1;
    cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
